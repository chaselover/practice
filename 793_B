import sys
input = sys.stdin.readline

for _ in range(int(input())):
    n = int(input())
    p = list(map(int, input().split()))
    index = {i:i for i in range(n)}
    for i in range(n):
        index[p[i]] = i
    for i in range(n):
        if p[i] != i:
            print(p[i] & p[index[i]])
            break
